echo "# Upstream Commits (git-upstream-changes)"
git-upstream-changes
echo
echo "# Local Branches"
git branch
echo
echo "# Local Commits (git-local-changes)"
git-local-changes
echo
echo "# Local Changed Files (git-local-changed-files)"
git-local-changed-files
echo
echo "# Local Changes (git status -s)"
git status -s
